-- WiFi Related Items
WiFiStatus OBJECT-TYPE
	SYNTAX		OBJECT IDENTIFIER
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"WiFi Information"
	::= { RoomGuard 1 }

SSID OBJECT-TYPE
	SYNTAX		DisplayString
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Configured SSID"
	::= { WiFiStatus 1 }
	
Status OBJECT-TYPE
	SYNTAX		DisplayString
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Current WiFi Status"
	::= { WiFiStatus 2 }
	
Signal OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"WiFi Signal Strength"
	::= { WiFiStatus 3 }


-- Sensor Items
Sensors	OBJECT-TYPE
	SYNTAX 		OBJECT IDENTIFIER
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"DHT Sensor inside RoomGuard"
	::= { RoomGuard 2 }
	
Sensor0	OBJECT-TYPE
	SYNTAX 		OBJECT IDENTIFIER
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"DHT Sensor inside RoomGuard"
	::= { Sensors 1 }

Temperature0 OBJECT-TYPE
	SYNTAX		INTEGER
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Temperature from Sensor 0"
	DEFVAL { 0 }
	::= { Sensor0 1 }

Temperature0 ::= TEXTUAL-CONVENTION
	DISPLAY-HINT	"d-1"
	STATUS			current
	DESCRIPTION
		"Temperature"
	SYNTAX INTEGER
	
Humidity0 OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Temperature from Sensor"
	DEFVAL { 0 }
	::= { Sensor0 2 }
	
System OBJECT-TYPE
	SYNTAX		OBJECT IDENTIFIER
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Scope for Experimental functions that are not part of main RoomGuard Repository"
	::= { RoomGuard 100 }
	
MemTotal OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Total RAM in Bytes"
	DEFVAL { 0 }
	::= { System 1 }

MemFree OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"Free RAM in Bytes"
	DEFVAL { 0 }
	::= { System 2 }
	
